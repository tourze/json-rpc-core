{
  "name": "tourze/json-rpc-core",
  "description": "JSON-RPC 2.0 Core",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "tourze/arrayable": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPC\\Core\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\JsonRPC\\Core\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit"
  }
}